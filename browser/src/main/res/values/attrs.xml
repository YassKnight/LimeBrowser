<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LimeBrowser">
        <!--        显示头部布局，默认显示。true：显示   false：隐藏-->
        <attr name="showTopbar" format="boolean" />
        <!--        头部布局中标题-->
        <attr name="titleText" format="string" />
        <!--        头部布局中图标-->
        <attr name="titleIcon" format="reference" />


        <!--        自定义头部布局-->
        <!--        <attr name="customTopbarLayout" format="reference" />-->
        <!--        自定义底部布局-->
        <!--        <attr name="customBottombar" format="reference" />-->


        <!--        显示底部返回按钮-->
        <attr name="showGobackBtn" format="boolean" />
        <!--        显示底部前进按钮-->
        <attr name="showGoForwardBtn" format="boolean" />



    </declare-styleable>
</resources>